@article{sheraziComprehensiveSurveyRF2022,
  title = {A {{Comprehensive Survey}} on {{RF Energy Harvesting}}: {{Applications}} and {{Performance Determinants}}},
  shorttitle = {A {{Comprehensive Survey}} on {{RF Energy Harvesting}}},
  author = {Sherazi, Hafiz Husnain Raza and Zorbas, Dimitrios and O'Flynn, Brendan},
