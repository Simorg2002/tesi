}

@article{lanHighefficientIntelligentAntibacterial2024,
  title = {High-Efficient and Intelligent Antibacterial Face Mask Integrated with Airflow-Temperature Dual-Function Sensors for Respiratory Monitoring and Disease Prevention},
  author = {Lan, Xingzi and Chen, Xinyu and Chen, Xin and Fan, Hao and Zheng, Hehui and Wang, Han and Tang, Yadong},
  year = {2024},
