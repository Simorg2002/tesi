  author = {Zou, Haiyang and Zhang, Ying and Guo, Litong and Wang, Peihong and He, Xu and Dai, Guozhang and Zheng, Haiwu and Chen, Chaoyu and Wang, Aurelia Chi and Xu, Cheng and Wang, Zhong Lin},
