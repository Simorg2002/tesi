@article{jiangSelfgeneratedElectricitydrivenDrug2024,
  title = {A Self-Generated Electricity-Driven Drug Delivery System for Precision Management of Myopia},
  author = {Jiang, Lingxi and Zhang, Luhan and Dai, Chao and Zhao, Bo and Yang, Yin and Wu, Zhengzheng and Qu, Chao and Zou, Liang and Lin, Zong-Hong and Miao, Yang-Bao and Shi, Yi},
