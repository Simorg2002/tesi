@article{sobianin3DPrintedPiezoelectricMicrodevice2024,
  title = {A {{3D-Printed Piezoelectric Microdevice}} for {{Human Energy Harvesting}} for {{Wearable Biosensors}}},
  author = {Sobianin, Ihor and Psoma, Sotiria D. and Tourlidakis, Antonios},
