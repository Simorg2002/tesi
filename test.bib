@article{ardiatnaMeasurementSpecificAbsorption2024,
  title = {Measurement of Specific Absorption Rate of Wearable Wireless Devices},
  author = {Ardiatna, Wuwus and Prananto, Haryo Dwi and Sukma, Irawan and Kasiyanto, Iput and Bakti, Aditia Nur and Supono, Ihsan and Wibowo, Priyo and Arjadi, R. Harry},
  year = {2024},
